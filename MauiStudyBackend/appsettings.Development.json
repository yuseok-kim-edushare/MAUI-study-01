{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Default": "Server=YOUR_SERVER;Database=YOUR_DATABASE;User Id=YOUR_USER;Password=YOUR_PASSWORD;TrustServerCertificate=True;"
  },
  "Auth": {
    "PasswordMode": "bcrypt"
  },
  "Jwt": {
    "Issuer": "MauiStudyBackend",
    "Audience": "MauiStudyApp",
    "Key": "ChangeThisInProduction_UseAtLeast32Characters",
    "TokenLifetimeMinutes": 60
  }
}
